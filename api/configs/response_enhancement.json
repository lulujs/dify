{
  "global": {
    "enabled": true,
    "default_processors": ["metadata"],
    "fail_silently": true,
    "log_level": "INFO"
  },
  "endpoints": [
    {
      "pattern": "/v1/completion-messages",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "json",
        "method": "POST"
      }
    },
    {
      "pattern": "/v1/chat-messages",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "json",
        "method": "POST"
      }
    },
    {
      "pattern": "/v1/audio-to-text",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "json"
      }
    },
    {
      "pattern": "/v1/text-to-audio",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "binary"
      }
    },
    {
      "pattern": "/v1/files",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "json"
      }
    },
    {
      "pattern": "/v1/files/*",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "json"
      }
    },
    {
      "pattern": "/v1/workflows/run",
      "processors": ["standard_format"],
      "enabled": true,
      "conditions": {
        "response_type": "json"
      }
    },
    {
      "pattern": "/v1/parameters",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "json"
      }
    },
    {
      "pattern": "/v1/suggested-questions",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "json"
      }
    },
    {
      "pattern": "/v1/*/stop",
      "processors": [],
      "enabled": false
    },
    {
      "pattern": "/v1/health",
      "processors": [],
      "enabled": false
    },
    {
      "pattern": "/v1/status",
      "processors": [],
      "enabled": false
    },
    {
      "pattern": "/v1/*/stream",
      "processors": ["metadata"],
      "enabled": true,
      "conditions": {
        "response_type": "streaming"
      }
    },
    {
      "pattern": "*",
      "processors": ["metadata"],
      "enabled": true
    }
  ]
}